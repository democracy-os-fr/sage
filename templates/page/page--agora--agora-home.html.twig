{% extends "page.html.twig" %}

{# Main #}
{% block main %}
  <div role="main" class="main-container js-quickedit-main-content">

    {# Header #}
    {% if page.header %}
      {% block header %}
        <section class="{{ container }}" role="heading">
          {{ page.header }}
        </section>
      {% endblock %}
    {% endif %}

    {# Content #}
    {% block content %}
      <a id="main-content"></a>
      {{ page.content }}
    {% endblock %}

  </div>
{% endblock %}
