{
  "name": "roots/sage",
  "type": "drupal-theme",
  "license": "MIT",
  "description": "Drupal starter theme with a modern development workflow",
  "homepage": "https://www.drupal.org/project/bootstrap",
  "authors": [
    {
      "name": "moustachu",
      "email": "moustachu@democracyos.eu",
      "homepage": "https://github.com/moustachu"
    }
  ],
  "keywords": [
    "drupal"
  ],
  "support": {
    "issues": "https://github.com/democracy-os-fr/sage/issues"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "autoload": {
    "psr-4": {
      "Roots\\Sage\\": "src/lib/Sage/"
    }
  },
  "require": {
    "php": ">=5.5.0",
    "drupal-composer/drupal-scaffold": "^2.0.1",
    "composer/installers": "~1.0",
    "symfony/dependency-injection": "2.8.*",
    "drupal/bootstrap": "3.x-dev",
    "drupal/adminimal_theme": "^1.2",
    "drupal/adminimal_admin_toolbar": "^1.3",
    "drupal/asset_injector": "^1.0-rc1",
    "drupal/views_timelinejs": "^3.0.0",
    "drupal/field_group": "^1.0-rc4",
    "drupal/search_kint": "^1.0",
    "drupal/twig_field_value": "^1.1",
    "ccampbell/chromephp": "^4.1",
    "drupal/browsersync": "1.x-dev",
    "drupal/email_registration": "1.x-dev",
    "drupal/simple_oauth": "1.x-dev",
    "drupal/restui": "^1.11",
    "drupal/paragraphs": "^1.0",
    "drupal/twig_tweak": "^1.3",
    "drupal/image_effects": "^1.0-alpha2",
    "drupal/color_field": "^2.0-rc1",
    "drupal/geofield": "1.x-dev",
    "leaflet/leaflet": "^0.7.7",
    "drupal/leaflet": "1.x-dev",
    "drupal/geocoder": "2.x-dev",
    "drupal/address": "1.x-dev",
    "drupal/yamlform": "1.x-dev",
    "drupal/token_filter": "1.x-dev",
    "drupal/image_url_formatter": "1.x-dev",
    "drupal/panels": "3.0.0-beta5",
    "drupal/devel": "1.x-dev",
    "drupal/entity": "1.0.0-alpha3",
    "drupal/realistic_dummy_content": "1.x-dev",
    "jquery-simple-color/jquery-simple-color": "^1.2",
    "views-timelinejs/TimelineJS3": "^3.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1",
    "phpmd/phpmd": "^2.4.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "test": [
      "vendor/bin/phpcs",
      "vendor/bin/phpmd src text cleancode,codesize,controversial,design,naming,unusedcode"
    ]
  },
  "repositories": {
    "drupal": {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
    },
    "leaflet": {
      "type": "package",
      "package": {
        "name": "leaflet/leaflet",
        "version": "0.7.7",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/Leaflet/Leaflet/archive/v0.7.7.zip",
          "type": "zip"
        }
      }
    },
    "jquery-simple-color": {
      "type": "package",
      "package": {
        "name": "jquery-simple-color/jquery-simple-color",
        "version": "1.2.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/recurser/jquery-simple-color/archive/v1.2.1.zip",
          "type": "zip"
        }
      }
    },
    "TimelineJS3": {
      "type": "package",
      "package": {
        "name": "views-timelinejs/TimelineJS3",
        "version": "3.4.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/NUKnightLab/TimelineJS3/archive/3.4.1.zip",
          "type": "zip"
        }
      }
    }
  },
  "config": {
      "vendor-dir": "../../../../vendor"
  },
  "extra": {
    "installer-paths": {
        "../../../../web/core": ["type:drupal-core"],
        "../../../../web/libraries/{$name}": ["type:drupal-library"],
        "../../../../web/modules/contrib/{$name}": ["type:drupal-module"],
        "../../../../web/profiles/contrib/{$name}": ["type:drupal-profile"],
        "../../../../web/themes/contrib/{$name}": ["type:drupal-theme"],
        "../../../../drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "patches": {
        "drupal/metatag": {
            "Deactivate console service": "https://www.drupal.org/files/issues/metatag-deactivate_drupalconsole_services_0.patch"
        },
        "drupal/drupal": {
            "Twig filter : Remove HTML Debug Comments": "https://www.drupal.org/files/issues/2672656-9.patch"
        },
        "drupal/color_field": {
            "Notice: Undefined index: #required": "https://www.drupal.org/files/issues/fixed-notice-2642712-2.patch"
        }


    }
  }
}
